generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Book {
  id       Int      @id @default(autoincrement())
  title    String
  author   Author   @relation(fields: [authorId], references: [id])
  authorId Int
}

model Author {
  id    Int     @id @default(autoincrement())
  name  String
  books Book[]
}